class Solution {
    public int findMaxLength(int[] nums) {
        if(nums.length == 0||nums =null) return 0;
        HashMap<Integer, Integer> map = new HashMap<>();//sum:index
        int max = 0;
        int count = 0;
        map.put(0,-1);
        for(int i = 0;i< nums.length;i++){
            if(nums[i]==0){
                count --;
                
            }else{
                count++;
            }
            if(map.containsKey())
        }
        
    }
}